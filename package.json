{
  "name": "expo-router-multiple-layouts",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "lint": "eslint .",
    "start": "expo start",
    "test": "jest",
    "ts:check": "tsc",
    "web": "expo start --web"
  },
  "prettier": {
    "bracketSameLine": true,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "eslintConfig": {
    "extends": [
      "universe",
      "universe/shared/typescript-analysis"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx",
          "*.d.ts"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        }
      }
    ],
    "root": true
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "expo": "~50.0.13",
    "expo-constants": "~15.4.5",
    "expo-font": "~11.10.3",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.8",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.5",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.3",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.0"
  }
}
